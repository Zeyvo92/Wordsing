{
  "name": "worker-lyrics",
  "version": "1.0.0",
  "description": "Worker for lyrics",
  "main": "build/index.js",
  "repository": "https://github.com/Zeyvo92/Wordsing",
  "license": "MIT",
  "scripts": {
    "build": "rimraf build && tsc",
    "deploy": "npm run build && export $(cat .env | xargs) && npx sls deploy",
    "offline:flush:song": "aws dynamodb delete-table --table-name \"lyrics\" --endpoint-url http://localhost:8000 && node script/create-dynamodb-table.js",
    "offline": "yarn build && npx sls offline start"
  },
  "dependencies": {
    "aws-sdk": "^2.1058.0",
    "axios": "^0.24.0",
    "cheerio": "^1.0.0-rc.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.89",
    "@types/aws-sdk": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.30",
    "@types/node": "^17.0.9",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "serverless": "^2.72.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^8.3.1",
    "serverless-offline-sqs": "^6.0.0",
    "typescript": "^4.5.4"
  }
}
